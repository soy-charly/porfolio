{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/aboutme",
      "dest": "/pages/aboutme.html"
    },
    {
      "src": "/knowledge",
      "dest": "/pages/knowledge.html"
    },
    {
      "src": "/portfolio",
      "dest": "/pages/portfolio.html"
    },
    {
      "src": "/contact",
      "dest": "/pages/contact.html"
    },
    {
      "src": "/styles/(.*)",
      "dest": "/styles/$1"
    }
  ]
}
